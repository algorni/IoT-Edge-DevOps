{
  "$schema-template": "1.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "acriotdevops": {
                "username": "$CONTAINER_REGISTRY_USERNAME_acriotdevops",
                "password": "$CONTAINER_REGISTRY_PASSWORD_acriotdevops",
                "address": "acriotdevops.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": {}
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "5671/tcp": [
                      {
                        "HostPort": "5671"
                      }
                    ],
                    "8883/tcp": [
                      {
                        "HostPort": "8883"
                      }
                    ],
                    "443/tcp": [
                      {
                        "HostPort": "443"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "modules": {
          "HttpVideoFrameCaptureModule": {
            "version": "1.0.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.HttpVideoFrameCaptureModule}",
              "createOptions": {}
            },
            "env": {
              "IMAGE_PROCESSING_ENDPOINT": {
                "value": "http://customvisionmodule:80/image"
              },
              "IMAGE_SOURCE_URL": {
                "value": "$IMAGE_SOURCE_URL"
              },
              "IMAGE_POLLING_INTERVAL": {
                "value": "30"
              },
              "MODE": {
                "value": "$MODE"
              },
              "CUSTOM_VISION_TRAINING_ApiKey": {
                "value": "$CUSTOM_VISION_TRAINING_ApiKey"
              },
              "CUSTOM_VISION_TRAINING_EndPoint": {
                "value": "$CUSTOM_VISION_TRAINING_EndPoint"
              },
              "CUSTOM_VISION_TRAINING_ProjectId": {
                "value": "$CUSTOM_VISION_TRAINING_ProjectId"
              },
              "IP_CAMERA_CREDENTIAL_UserName": {
                "value": "$IP_CAMERA_CREDENTIAL_UserName"
              },
              "IP_CAMERA_CREDENTIAL_Password": {
                "value": "$IP_CAMERA_CREDENTIAL_Password"
              }
            }
          },
          "customvisionmodule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.CustomVisionModule}",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "80/tcp": [
                      {
                        "HostPort": "80"
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "httpVideoFrameCaptureModuleToIoTHub": "FROM /messages/modules/HttpVideoFrameCaptureModule/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}